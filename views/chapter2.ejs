<section class="ch-2">

  <div id="view-chapter2">
    <button id="school-start">START</button>
    <div class="info-box">
      <div class="chtitle"></div>
    </div>
    <button id="school-prev"><img src="/images/Chapter2-ENG/group-431.svg" alt=""></button>
